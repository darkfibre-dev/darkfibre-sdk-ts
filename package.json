{
  "name": "@darkfibre/sdk",
  "version": "0.0.5",
  "publishConfig": {
    "access": "public"
  },
  "description": "TypeScript SDK for Darkfibre Solana DEX API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test:e2e": "npx tsx tests/e2e/run-e2e.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "solana",
    "dex",
    "darkfibre",
    "trading",
    "sdk"
  ],
  "author": "Darkfibre",
  "license": "BUSL-1.1",
  "dependencies": {
    "@solana/kit": "^6.0.1",
    "axios": "^1.13.4",
    "bs58": "^6.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^20.19.32",
    "dotenv": "^17.2.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
